doctype 5
html
  head
    title= page.title
    script(src='/js/jquery-1.72.min.js')
    script(src='/js/bootstrap.js')
    link(rel='stylesheet', href='/css/bootstrap.css')
    link(rel='stylesheet', href='/css/style.css')
  body
    if adminUser
      include navbar
    #page
      header
        hgroup
          h1= blog.title
          h2= blog.description
      #main
        block content
      footer
        p generated by nblog.
